-- รอเกมโหลดครบ
repeat task.wait() until game:IsLoaded()

-- ฟังก์ชันเช็ค GUI ว่ามีครบหรือไม่
local function hasFishingGUI()
    local player = game:GetService("Players").LocalPlayer
    local gui = player:FindFirstChild("PlayerGui")
    if not gui then return false end

    local interface = gui:FindFirstChild("Interface")
    if not interface then return false end

    local fishingFrame = interface:FindFirstChild("FishingCatchFrame")
    if not fishingFrame then return false end

    local timingBar = fishingFrame:FindFirstChild("TimingBar")
    if not timingBar then return false end

    local successArea = timingBar:FindFirstChild("SuccessArea")
    if not successArea then return false end

    return true
end

-- เลือกลิงก์สคริปต์ตามผลเช็ค
local url
if hasFishingGUI() then
    url = "https://raw.githubusercontent.com/Chxtoqfee12/_99Night/refs/heads/main/99night"
else
    url = "https://raw.githubusercontent.com/Chxtoqfee12/_99Night/refs/heads/main/Outside"
end

-- โหลดและรันสคริปต์
loadstring(game:HttpGet(url))()
